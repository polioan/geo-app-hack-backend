{
  "name": "geo-app-hack-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dprune": "docker system prune -a -f --volumes",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node dist/main",
    "dev": "cross-env NODE_ENV=development tsx --watch src/main",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "dep": "npm ci && npm run build && pm2 start npm --name \"myapp\" -- start --watch"
  },
  "dependencies": {
    "@polioan/common": "^0.0.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-status": "^1.7.3",
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.15",
    "@types/csvtojson": "^2.0.0",
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.7",
    "@types/qrcode": "^1.5.4",
    "prettier": "^3.0.3",
    "prettier-plugin-jsdoc": "^1.1.1",
    "rimraf": "^5.0.5",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2"
  }
}
